<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:key="preview_size"
        android:title="preview resolution " />
    <ListPreference
        android:key="picture_size"
        android:title="photo resolution" />
    <ListPreference
        android:key="video_size"
        android:title="video resolution" />
    <SwitchPreference
        android:defaultValue="true"
        android:key="gps_data"
        android:title="location" />
    <ListPreference
        android:defaultValue="auto"
        android:key="flash_mode"
        android:title="Flash" />
    <ListPreference
        android:key="focus_mode"
        android:title="focus_mode" />
    <ListPreference
        android:defaultValue="auto"
        android:key="white_balance"
        android:title="White balance" />
    <ListPreference
        android:defaultValue="auto"
        android:key="scene_mode"
        android:title="Scene" />
    <ListPreference
        android:defaultValue="0"
        android:key="exposure_compensation"
        android:title="Exposure Compensation" />
    <ListPreference
        android:defaultValue="100"
        android:entries="@array/pref_jpegQuality"
        android:entryValues="@array/pref_jpegQuality"
        android:key="jpeg_quality"
        android:title="Photos Quality" />
</PreferenceScreen>